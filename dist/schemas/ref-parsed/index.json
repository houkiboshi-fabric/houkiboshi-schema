          "examples": [
            "rakuda",
            "yama-no-odori",
            "akane-ni-tachibana",
            "sakeori-no-kaban"
          ],
          "type": "string",
          "pattern": "^[a-z0-9\\-]+$"
        },
        "name": {
          "description": "Product の銘となる名前です。",
          "examples": [
            "駱駝",
            "山の踊り",
            "茜に橘",
            "裂織のかばん"
          ],
          "type": "string"
        },
        "name_kana": {
          "description": "Product の name の読み仮名です。\n全角平仮名と半角数字のみ使用できます。",
          "examples": [
            "らくだ",
            "やまのおどり",
            "あかねにたちばな",
            "さきおりのかばん"
          ],
          "type": "string",
          "pattern": "^[0-9ぁ-んー]+$"
        },
        "name_alt": {
          "type": "string",
          "description": "Product の別名です。\n省略可能です。\nname_alt が存在する Product は name_alt_kana も必須プロパティになります。",
          "examples": [
            "草木染め糸のストール",
            "飾る布",
            "草木染色腹帯"
          ]
        },
