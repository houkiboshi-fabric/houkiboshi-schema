      "title": "Product",
      "description": "製品です。",
      "type": "object",
      "required": [
        "id",
        "name",
        "name_kana",
        "image",
        "size",
        "weight_g",
        "raw_materials",
        "dyeing_materials"
      ],
      "additionalProperties": false,
      "dependencies": {
        "name_alt": [
          "name_alt_kana"
